{
  "name": "gridvue",
  "version": "0.0.1",
  "license": "MIT",
  "main": "index.js",
  "repository": "https://github.com/suranyami/gridvue",
  "dependencies": {
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.2"
  },
  "scripts": {
    "build": "GIT_SHA=`git rev-parse HEAD` webpack --optimize-minimize --colors --output-path dist/",
    "release": "NODE_ENV=production GIT_SHA=`git rev-parse HEAD` webpack --optimize-minimize --colors --output-path dist/",
    "eslint": "eslint build",
    "start": "webpack-dev-server --host 0.0.0.0 --content-base build",
    "test": "karma start spec/karma.conf.js",
    "test-dev": "karma start spec/karma.conf.dev.js",
    "s": "webpack-dev-server --host 0.0.0.0 --content-base build --watch-poll"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "style-loader": "^0.13.1",
    "vue-hot-loader": "0.0.3",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3"
  }
}
